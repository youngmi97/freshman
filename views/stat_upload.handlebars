<form method="post" action="/users/upload/stat_upload">
   <div class="form-group">
    <label>Date</label>
    <input type="text" class="form-control" placeholder="Date" name="date">
  </div>
  <div class="form-group">
    <label>Game</label>
    <input type="text" class="form-control" placeholder="Game" name="game">
  </div>
   <div class="form-group">
    <label>Name</label>
    <input type="text" class="form-control" placeholder="Name" name="name">
  </div>
  <div class="form-group">
    <label>Points</label>
    <input type="text" class="form-control" placeholder="Points" name="points">
  </div>
  <div class="form-group">
    <label>Rebounds</label>
    <input type="text" class="form-control" placeholder="Rebounds" name="rebounds">
  </div>
  <div class="form-group">
    <label>Assists</label>
    <input type="text" class="form-control" placeholder="Assists" name="assists">
  </div>
  <div class="form-group">
    <label>Steals</label>
    <input type="text" class="form-control" placeholder="Steals" name="steals">
  </div>
  <div class="form-group">
    <label>Blocks</label>
    <input type="text" class="form-control" placeholder="Blocks" name="blocks">
  </div>
  <div class="form-group">
    <label>Turnovers</label>
    <input type="text" class="form-control" placeholder="Turnovers" name="turnovers">
  </div>
  <div class="form-group">
    <label>Fouls</label>
    <input type="text" class="form-control" placeholder="Fouls" name="fouls">
  </div>
  <button type="submit" class="btn btn-default">Upload</button>
</form>